{
  "extends": ["tslint:recommended", "tslint-angular", "tslint-config-prettier"],
  "rules": {
    "interface-name": false,
    "no-output-on-prefix": false,
    "directive-selector": [
      true,
      "attribute",
      "",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ]
  }
}
